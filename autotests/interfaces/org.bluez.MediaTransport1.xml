<?xml version="1.0"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.bluez.MediaTransport1">
    <method name="Acquire">
      <arg name="fileDescriptor" type="h" direction="out"/>
      <arg name="readMtu" type="q" direction="out"/>
      <arg name="writeMtu" type="q" direction="out"/>
    </method>
    <method name="TryAcquire">
      <arg name="fileDescriptor" type="h" direction="out"/>
      <arg name="readMtu" type="q" direction="out"/>
      <arg name="writeMtu" type="q" direction="out"/>
    </method>
    <method name="Release"/>
    <property name="Codec" type="y" access="read"/>
    <property name="Configuration" type="ay" access="read"/>
    <property name="State" type="s" access="read"/>
    <property name="Volume" type="q" access="read"/>
  </interface>
</node>
